<?xml version="1.0" encoding="ISO-8859-1" ?>
<!-- do not use the "Type" attribute here, that one is only for
     generated package.xml files -->
<package>
  <name>HTML_QuickForm</name>
  <summary>The PEAR::HTML_QuickForm package provides methods for creating, validating, processing HTML forms.</summary>
  <maintainer>
    <user>mansion</user>
    <name>Bertrand Mansion</name>
    <email>bmansion@mamasam.com</email>
  </maintainer>
  
  <release>
    <version>1.0</version>
    <date>2001-09-06</date>
    <notes>
      This is the initial release of the HTML_QuickForm package.
    </notes>
  </release>
  
  <filelist>
    <dir name="/" baseinstalldir="HTML">
        <dir name="QuickForm">
            <file role="php">QuickForm.php</file>
            <dir name="Elements">
                <file role="php">button.php</file>
                <file role="php">checkbox.php</file>
                <file role="php">date.php</file>
                <file role="php">element.php</file>
                <file role="php">file.php</file>
                <file role="php">group.php</file>
                <file role="php">hidden.php</file>
                <file role="php">image.php</file>
                <file role="php">input.php</file>
                <file role="php">password.php</file>
                <file role="php">radio.php</file>
                <file role="php">reset.php</file>
                <file role="php">select.php</file>
                <file role="php">submit.php</file>
                <file role="php">text.php</file>
                <file role="php">textarea.php</file>
            </dir>
            <dir name="docs" role="doc">
                <file>QuickForm_example.php</file>
            </dir>
        </dir>
    </dir>
  </filelist>
</package>